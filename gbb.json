{
    "version": "0.6.1",
    "tool": "go build -v -trimpath -ldflags '-s -w'",
    "importpath": "github.com/voidint/g/internal/build",
    "variables": [
        {
            "variable": "Build",
            "value": "{{.Date}}"
        },
        {
            "variable": "Commit",
            "value": "$(git log -1 | awk 'NR==1 {print substr($2,0,8)}')"
        },
        {
            "variable": "Branch",
            "value": "$(git symbolic-ref --short -q HEAD)"
        }
    ]
}